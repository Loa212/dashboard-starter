{
  "name": "dashboard-starter",
  "description": "dashboard starter project using Better Auth, TanStack, React, Vite, shadcn/ui and TailwindCSS",
  "version": "0.1.0",
  "author": "Loa212",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build && tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check --write",
    "typecheck": "tsc --noEmit",
    "db:generate": "pnpm dlx @better-auth/cli@latest generate",
    "db:migrate": "pnpm dlx @better-auth/cli@latest migrate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tabler/icons-react": "^3.36.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.0",
    "@tanstack/nitro-v2-vite-plugin": "^1.154.7",
    "@tanstack/react-devtools": "^0.9.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.158.0",
    "@tanstack/react-router-devtools": "^1.158.0",
    "@tanstack/react-start": "^1.158.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-plugin": "^1.158.0",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^22.19.8",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^27.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}